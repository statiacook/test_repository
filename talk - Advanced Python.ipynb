    {
     "cell_type": "code",
     "collapsed": false,
     "input": [
      "import logging, sys\n",
      "\n",
      "# create the logger\n",
      "logger = logging.getLogger(\"lumberjack\")\n",
      "\n",
      "# bind a handler, in this case a stream handler that writes to stdout\n",
      "sh = logging.StreamHandler(stream=sys.stdout)\n",
      "\n",
      "# define a formatter for the output messages\n",
      "fmt = logging.Formatter('[%(levelname)s]: %(message)s')\n",
      "\n",
      "sh.setFormatter(fmt)\n",
      "logger.handlers = [sh]"
     ],
     "language": "python",
     "metadata": {},
     "outputs": []
    },
    {
     "cell_type": "code",
     "collapsed": false,
